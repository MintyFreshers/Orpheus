{
  "Discord": {
    "Token": "<YOUR_DISCORD_BOT_TOKEN_HERE>",
    "DefaultChannelId": "<YOUR_DISCORD_DEFAULT_CHANNEL_ID_HERE>"
  },
  "PicovoiceAccessKey": "<YOUR_PICOVOICE_ACCESS_KEY_HERE>",
  "WakeWord": {
    "EnabledWords": "computer,orpheus",
    "DetectionCooldownMs": 3000,
    "Sensitivities": {
      "computer": 0.6,
      "orpheus": 0.7,
      "jarvis": 0.6,
      "bumblebee": 0.6
    },
    "CustomModels": {
      "orpheus": "Resources/orpheus_keyword_file.ppn"
    }
  },
  "AzureSpeech": {
    "SubscriptionKey": "<YOUR_AZURE_SPEECH_SUBSCRIPTION_KEY_HERE>",
    "Region": "eastus"
  },
  "Cache": {
    "MaxFiles": 100,
    "MaxSizeBytes": 1073741824,
    "CacheDirectory": "/data/cache",
    "StorageType": "Sqlite",
    "EnableAutomaticCleanup": true,
    "CleanupIntervalMinutes": 60
  }
}